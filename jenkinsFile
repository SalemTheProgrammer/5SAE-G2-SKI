pipeline {
    agent any

    stages {
        stage('GIT') {
            steps {
                git branch: 'INESRDISSI-5SAE7-G2',
                    url: 'https://github.com/SalemTheProgrammer/5SAE-G2-SKI.git'
            }
        }
        
        stage('Compile Stage') {
            steps {
                sh 'mvn clean compile'
            }
        }
        
  

      stage('Nexus') {
            steps {
             sh 'mvn deploy -Dmaven.test.skip=true';
           }
       }
           

    }
}
